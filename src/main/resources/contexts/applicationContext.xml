<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.shreyash.demo.Sms" name="sms" id = "sms_bean">
    </bean>

    <bean class="com.shreyash.demo.Notification" name="notification" id = "notification">
        <property name="thisService">
            <bean id="whatsapp" class="com.shreyash.demo.Whatsapp"/>
        </property>
    </bean>


</beans>